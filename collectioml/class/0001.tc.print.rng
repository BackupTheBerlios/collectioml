<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" xmlns="http://relaxng.org/ns/structure/1.0"
  ns="http://collectio.org/classifications/">
  <define name="techniqueclass-prints">
    <element name="techniqueclass">
      <documentation xmlns="http://relaxng.org/ns/compatibility/annotations/1.0">Coverage: modern times.</documentation>
      <attribute name="authority">
        <value>collectio:0001</value>
      </attribute>
      <attribute name="coverage">
        <value>print media</value>
      </attribute>
        <element name="method">
          <choice>
            <value>aquatint</value>
            <value>chromolithography</value>
            <value>engraving</value>
            <value>etching</value>
            <value>half-tone</value>
            <value>letterpress</value>
            <value>linocut</value>
            <value>lithography</value>
            <value>mezzotint</value>
            <value>offset</value>
            <value>photolithography</value>
            <value>silk screen printing</value>
            <value>xerocopy</value>
            <value>xylography</value>
          </choice>
        </element>
    </element>
  </define>
</grammar>
