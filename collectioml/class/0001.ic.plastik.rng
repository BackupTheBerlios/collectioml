<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" xmlns="http://relaxng.org/ns/structure/1.0"
  ns="http://collectio.org/classifications/">
  <define name="iconclass-plastik">
    <element name="iconclass" ns="http://collectio.org/classifications/">
      <documentation xmlns="http://relaxng.org/ns/compatibility/annotations/1.0">Coverage: mediterranean area, classical roman and greek
        times.</documentation>
      <attribute name="authority">
        <value>collectio:0001</value>
      </attribute>
      <attribute name="coverage">
          <value>plastik</value>
      </attribute>
        <element name="type">
          <choice>
            <value>statuen</value>
            <value>reliefs</value>
          </choice>
        </element>
        <optional>
          <element name="subtype">
            <choice>
              <value>stehend</value>
              <value>sitzend</value>
              <value>liegend</value>
              <value>kauernd</value>
              <value>hockend</value>
            </choice>
          </element>
      </optional>
    </element>
  </define>
</grammar>
