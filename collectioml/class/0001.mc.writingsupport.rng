<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" xmlns="http://relaxng.org/ns/structure/1.0"
  ns="http://collectio.org/classifications/">
  <define name="materialclass-writingsupport">
    <element name="materialclass">
      <documentation xmlns="http://relaxng.org/ns/compatibility/annotations/1.0">Coverage: all times.</documentation>
      <attribute name="authority">
        <value>collectio:0001</value>
      </attribute>
      <attribute name="coverage">
        <value>writing supports</value>
      </attribute>
      <optional>
        <element name="type">
          <choice>
            <value>vellum</value>
            <value>parchment</value>
            <value>rag paper</value>
            <value>cellulose paper</value>
            <value>papyrus</value>
          </choice>
        </element>
      </optional>
    </element>
  </define>
</grammar>
