<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" xmlns="http://relaxng.org/ns/structure/1.0"
  ns="http://collectio.org/classifications/">
  <define name="techniqueclass-prints">
    <element name="techniqueclass">
      <documentation xmlns="http://relaxng.org/ns/compatibility/annotations/1.0">Cascading location classification schema for types of
        printing techniques. Very sketchy. A good glossary at http://www.ifla.org/VII/s30/pub/mg1.htm</documentation>
      <attribute name="coverage">
        <value>modern times</value>
      </attribute>
      <element name="technique">
        <value>print</value>
      </element>
      <optional>
        <element name="method">
          <choice>
            <value>aquatint</value>
            <value>engraving</value>
            <value>etching</value>
            <value>watercolour</value>
          </choice>
        </element>
      </optional>
    </element>
  </define>
</grammar>
