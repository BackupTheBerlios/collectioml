<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" xmlns="http://relaxng.org/ns/structure/1.0"
  ns="http://collectio.org/classifications/">
  <define name="objectclass-numismatic">
    <element name="objectclass">
      <documentation xmlns="http://relaxng.org/ns/compatibility/annotations/1.0">Classification schema for coins of
        the Roman Empire. This definition is not being used so far. Very sketchy.</documentation>
      <attribute name="coverage">
        <value>numismatic, roman late republic and imperial</value>
      </attribute>
      <element name="group">
          <value>numismatic</value>
      </element>
      <element name="material">
        <choice>
          <value>silver</value>
          <value>gold</value>
          <value>bronce</value>
        </choice>
      </element>
      <optional>
        <element name="denomination">
          <choice>
            <value>as</value>
            <value>sestertium</value>
          </choice>
        </element>
      </optional>
    </element>
  </define>
</grammar>
